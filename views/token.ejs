          <th> 테이블 네임 </th>
          <th> 컬럼 네임 </th>
          <th> 컬럼 설명 </th>
        </tr>
        <% for ( var i = 0; i < data.length; i++){ %>
          <tr style="border: 1px solid black;">
            <td><%=data[i].TABLE_NAME %></td>
            <td><%=data[i].COLUMN_NAME %></td>
            <td><%=data[i].COLUMN_COMMENT %></td>
          </tr>
